<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/d10"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/d10"
        android:text="@string/paper_normal_receipt"
        android:textSize="@dimen/d12"
        android:textColor="@color/text_normal" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/d10" />

    <FrameLayout
        android:id="@+id/container_paper_normal_receipt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/d10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/d10"
        android:text="@string/paper_special_receipt"
        android:textSize="@dimen/d12"
        android:textColor="@color/text_normal" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/d10" />

    <FrameLayout
        android:id="@+id/container_paper_special_receipt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </FrameLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/d10" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/d10"
        android:text="@string/paper_elec_receipt"
        android:textColor="@color/text_normal"
        android:textSize="@dimen/d12"
         />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/d10" />

    <FrameLayout
        android:id="@+id/container_paper_elec_receipt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </FrameLayout>

</LinearLayout>