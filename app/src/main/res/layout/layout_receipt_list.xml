<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_receiptlist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/d10"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/ll_container_paper_normal_receipt"

            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_num_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d10"
                android:text="@string/paper_normal_receipt"
                android:textSize="@dimen/d12"
                android:textColor="@color/text_normal" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/d10" />

            <FrameLayout
                android:id="@+id/container_paper_normal_receipt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"></FrameLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_container_paper_special_receipt"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/d10" />

            <TextView
                android:id="@+id/tv_num_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d10"
                android:text="@string/paper_special_receipt"
                android:textSize="@dimen/d12"
                android:textColor="@color/text_normal" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/d10" />

            <FrameLayout
                android:id="@+id/container_paper_special_receipt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
            </FrameLayout>


        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_container_paper_elec_receipt"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/d10" />

            <TextView
                android:id="@+id/tv_num_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/d10"
                android:text="@string/paper_elec_receipt"
                android:textColor="@color/text_normal"
                android:textSize="@dimen/d12"
                />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/d10" />

            <FrameLayout
                android:id="@+id/container_paper_elec_receipt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </FrameLayout>


        </LinearLayout>




    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_no_record"
        android:orientation="vertical"
        android:layout_margin="@dimen/d10"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:src="@mipmap/no_list"/>
        <TextView
            android:layout_marginTop="@dimen/d20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/no_receipt"/>
    </LinearLayout>


</FrameLayout>