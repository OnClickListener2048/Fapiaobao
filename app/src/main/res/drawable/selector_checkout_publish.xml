<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="false">
    <shape android:shape="rectangle">
        <corners
            android:bottomLeftRadius="@dimen/d4"
            android:bottomRightRadius="@dimen/d4"
            android:radius="@dimen/d4"
            android:topLeftRadius="@dimen/d4"
            android:topRightRadius="@dimen/d4" />
        <solid android:color="#facd89" />
    </shape>
</item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="@dimen/d4"
                android:bottomRightRadius="@dimen/d4"
                android:radius="@dimen/d4"
                android:topLeftRadius="@dimen/d4"
                android:topRightRadius="@dimen/d4" />
            <solid android:color="#aafacd89" />
        </shape>
    </item>
</selector>